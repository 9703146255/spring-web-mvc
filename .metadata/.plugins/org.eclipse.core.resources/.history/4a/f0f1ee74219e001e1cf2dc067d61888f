@Controller
public class UserController {
	
	public ModelAndView getUserData()
	{
	ModelAndView mav=new ModelAndView();
	mav.addObject("ID :: ",1001);
	mav.addObject("NAME :: ","THIRUMALESH");
	
	
	
	
	
		return mav;
	}

}
